// Fizbuzz made easy
var output = [];
var count = 1;

// A function for the fizzbuzz project
function fizzBuzz(){
    
if (count % 3 === 0 && count % 5 === 0){
    output.push("Fizzbuzz");
}else if( count % 3=== 0){
    output.push("Fizz");
    }
else if (count % 5 === 0){
    output.push("Bizz");
}
else{
    output.push(count)
}
count++;
    console.log(output);
}
